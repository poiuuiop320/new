<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
<bean id="b1" class="com.kic.set.BeanSet2">
<constructor-arg>
<set>
<!-- MemberDTO 객체 3개를 만들어서 프로퍼티에 값을 넣음-->
<bean class="com.kic.set.MemberDTO" p:name="hong" p:age="20"></bean>
<bean class="com.kic.set.MemberDTO" p:name="park" p:age="15"></bean>
<bean class="com.kic.set.MemberDTO" p:name="kim" p:age="29"></bean>
</set>
</constructor-arg>
</bean>

</beans>
